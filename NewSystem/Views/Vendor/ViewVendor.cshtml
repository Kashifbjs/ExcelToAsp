@model IEnumerable<Vendor>
@{
    ViewData["Title"] = "View Vendor";
}

<div class="container p-4">
    <h6 class="text-success text-center">
        @TempData["SuccessMsg"]
        @TempData["ErrorMsg"]
    </h6>
    <div class="row p-3">
        <div class="col-6">
            <h2>@ViewBag.VendorName</h2>
        </div>
    </div>
</div>


<a asp-action="Index" asp-controller="Vendor">Back to List</a>